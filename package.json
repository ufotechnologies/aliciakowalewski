{
  "name": "aliciakowalewski",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run generate && concurrently --names \"ROLLUP,SASS,HTTP\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"rollup -c -w -m inline\" \"sass src/scss:public/assets/css --embed-source-map --watch\" \"servez public\"",
    "build": "rollup -c && sass src/scss:public/assets/css --no-source-map --style compressed",
    "generate": "npm run build && node ./routes && node ./render",
    "start": "servez public"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "rollup": "^4.9.0",
    "rollup-plugin-bundleutils": "^2.0.1",
    "sass": "^1.69.5",
    "servez": "^2.1.2"
  },
  "dependencies": {
    "gsap": "^3.12.4"
  }
}
