{
  "name": "aliciakowalewski",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"ROLLUP,SASS,HTTP\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"rollup -c -w -m inline\" \"sass src/scss:public/assets/css --watch\" \"servez public\"",
    "build": "rollup -c && sass src/scss:public/assets/css",
    "generate": "npm run build && node ./routes && node ./render",
    "start": "servez public",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.51.0",
    "rollup": "^4.0.2",
    "rollup-plugin-bundleutils": "^2.0.0",
    "sass": "^1.69.3",
    "servez": "^2.1.1"
  },
  "dependencies": {
    "gsap": "^3.12.2"
  }
}
